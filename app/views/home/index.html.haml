%section.section-white
  .container
    #carousel-example-generic.carousel.slide{"data-ride" => "carousel"}
      / Indicators
      %ol.carousel-indicators
        %li.active{"data-slide-to" => "0", "data-target" => "#carousel-example-generic"}
        %li{"data-slide-to" => "1", "data-target" => "#carousel-example-generic"}
        %li{"data-slide-to" => "2", "data-target" => "#carousel-example-generic"}
      / Wrapper for slides
      .carousel-inner
        .item.active
          %img.img-responsive{:alt => "...", :src => "https://fbcdn-sphotos-c-a.akamaihd.net/hphotos-ak-xfa1/v/t1.0-9/1621794_1577603245856248_3626243330691755428_n.png?oh=3979b62a5450c3cfe33a011fb02c7207&oe=55D4EA93&__gda__=1439771301_ed5561b2c67ff335e5c32abd9db02c51"}/
          .carousel-caption
            %h2 Heading
        .item
          %img.img-responsive{:alt => "...", :src => "https://scontent.cdninstagram.com/hphotos-xfa1/t51.2885-15/e15/10808812_1635555523338293_846331485_n.jpg"}
            %h2 Heading
        .item
          %img.img-responsive{:alt => "...", :src => "https://fbcdn-sphotos-b-a.akamaihd.net/hphotos-ak-xtp1/v/t1.0-9/11062820_1592740147675891_4758577231455459716_n.jpg?oh=8db2ecac68a7160709b8feb4bcc05326&oe=55CBDE6B&__gda__=1439433293_54c5e939b1c26f75a9a50e3322faa71d"}/
          .carousel-caption
            %h2 Heading
      / Controls
      %a.left.carousel-control{"data-slide" => "prev", :href => "#carousel-example-generic"}
        %span.glyphicon.glyphicon-chevron-left
      %a.right.carousel-control{"data-slide" => "next", :href => "#carousel-example-generic"}
        %span.glyphicon.glyphicon-chevron-right